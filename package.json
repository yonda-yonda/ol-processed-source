{
  "name": "ol-processed-source",
  "version": "1.0.0",
  "description": "OpenLayers Processed Source",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "gatsby": "gatsby build --prefix-paths",
    "start": "gatsby develop",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "test": "jest",
    "lint": "eslint ./src/",
    "build": "tsc -p ./",
    "prepublishOnly": "yarn build",
    "deploy": "yarn gatsby && gh-pages -d public"
  },
  "author": "yonda <yonda.fountain@gmail.com>",
  "homepage": "https://github.com/yonda-yonda/ol-processed-source",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.6",
    "@mui/system": "^5.2.6",
    "@types/colormap": "^2.3.1",
    "@types/jest": "^27.5.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "assert": "^2.0.0",
    "babel-jest": "^28.1.0",
    "babel-preset-gatsby": "^2.13.0",
    "browserify-zlib": "^0.2.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby": "^4.24.4",
    "gatsby-plugin-material-ui": "^4.1.0",
    "gatsby-plugin-react-helmet-async": "^1.2.3",
    "gh-pages": "^4.0.0",
    "https-browserify": "^1.0.0",
    "jest": "^28.1.0",
    "prettier": "2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.37.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^28.0.1",
    "typescript": "^4.5.2",
    "url": "^0.11.0"
  },
  "dependencies": {
    "colormap": "^2.3.2",
    "geo4326": "^1.4.1",
    "geotiff": "^2.0.5",
    "ol": "^6.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonda-yonda/ol-processed-source.git"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!ol/.*)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/src/**/*.test.ts"
    ]
  },
  "keywords": [
    "typescript",
    "map",
    "ol",
    "geotiff"
  ]
}
