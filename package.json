{
  "name": "ol-processed-source",
  "version": "1.0.2",
  "description": "OpenLayers Processed Source",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "PORT=3000 craco start",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "deploy": "craco build; gh-pages -d build",
    "test": "jest",
    "build": "tsc --project tsconfig.build.json",
    "prepublishOnly": "yarn build"
  },
  "author": "yonda <yonda.fountain@gmail.com>",
  "homepage": "https://yonda-yonda.github.io/ol-processed-source",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@craco/craco": "^7.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/system": "^5.14.18",
    "@types/jest": "^29.5.10",
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "gh-pages": "^6.1.0",
    "jest": "^29.7.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.1",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "stylelint": "^14.14.1",
    "stylelint-config-recommended": "^9.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.88.2"
  },
  "dependencies": {
    "@types/colormap": "^2.3.1",
    "colormap": "^2.3.2",
    "geo4326": "^1.4.2",
    "geotiff": "^2.0.5",
    "ol": "^6.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonda-yonda/ol-processed-source.git"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ],
      "node_modules/ol/.+.(j|t)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!ol/)"
    ],
    "testMatch": [
      "**/src/**/*.test.ts"
    ]
  },
  "keywords": [
    "typescript",
    "map",
    "ol",
    "geotiff"
  ]
}
